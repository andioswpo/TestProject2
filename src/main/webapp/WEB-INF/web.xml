<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>CookieFilter</filter-name>
  	<display-name>CookieFilter</display-name>
  	<description></description>
  	<filter-class>com.gf.util.CookieFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>CookieFilter</filter-name>
  	<url-pattern>/CookieFilter</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>DownImgServlet</servlet-name>
  	<display-name>DownImgServlet</display-name>
  	<description></description>
  	<servlet-class>com.gf.statusflow.DownImgServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DownAttServlet</servlet-name>
  	<display-name>DownAttServlet</display-name>
  	<description></description>
  	<servlet-class>com.gf.util.DownAttServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WeiXinServlet</servlet-name>
  	<display-name>WeiXinServlet</display-name>
  	<description></description>
  	<servlet-class>com.gf.weixin.WeiXinServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DownImgServlet</servlet-name>
  	<url-pattern>/DownImgServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DownAttServlet</servlet-name>
  	<url-pattern>/DownAttServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WeiXinServlet</servlet-name>
  	<url-pattern>/WeiXinServlet</url-pattern>
  </servlet-mapping>
</web-app>
